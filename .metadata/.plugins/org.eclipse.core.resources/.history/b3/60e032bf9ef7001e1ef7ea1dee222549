package am;

public class Sub01 {
	public static void run() {
		boolean run = true;
		while(run) {
		System.out.println("\n--------------------------------------------");
		System.out.println("1.성적등록|2.성적목록|3.성적조회|4.성적삭제|0.종료");
		System.out.println("--------------------------------------------");
		break;
		}
	}
}
