package am;

import java.util.*;

public class Sub01 {
	public static void run() {
		ArrayList<Car> cars=new ArrayList<Car>();

		Car car1= new Car("기아", "k7");
		car1.setColor("흰색");
		car1.setDistance(100000);
		cars.add(car1);
		for(Car car:cars) {
			System.out.println(car.toString());
			System.out.println("");
		}
	}
}
